{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleDetection": "auto",
    "removeComments": true,
    "disableSizeLimit": true,
    "disableReferencedProjectLoad": true
  },
  "include": ["vite.config.ts"]
}
